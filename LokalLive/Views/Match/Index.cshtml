@model IEnumerable<LokalLive.Models.Match>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MatchId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MatchName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MatchStart)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MatchEnd)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IntermissionStart)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IntermissionEnd)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MatchId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MatchName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MatchStart)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MatchEnd)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IntermissionStart)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IntermissionEnd)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MatchId }) |
            @Html.ActionLink("Details", "Details", new { id=item.MatchId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MatchId })
        </td>
    </tr>
}

</table>
